{
    "extends": "airbnb",
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "max-len": [
            2,
            200
        ],
        "import/no-unresolved": "off",
        "import/extensions":"off",
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 2,
                "maxBOF": 0,
                "maxEOF": 0
            }
        ],
        "@typescript-eslint/no-unused-vars": 2
    }
}
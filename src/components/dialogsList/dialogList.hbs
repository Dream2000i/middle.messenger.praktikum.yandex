{{#each dialogs}}
<div class="dialog__item {{itemClass}}" data-dialog-id="{{dialogId}}">
    <div class="avatar">
        <img src="{{this.avatar}}" alt="Аватар пользователя {{this.nickname}}">
    </div>
    <div class="nickname">
        {{this.nick}}
    </div>
    <div class="time_last_message">
        {{this.timeLastMessage}}
    </div>
    <div class="last_message">
        {{{this.lastMessage}}}
    </div>
    <div class="count_new_message">
        {{#if this.countNewMessage}}
        <div>
            {{{this.countNewMessage}}}
        </div>
        {{/if}}
    </div>
</div>
{{/each}}